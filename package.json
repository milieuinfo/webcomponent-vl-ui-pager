{
  "name": "vl-ui-pager",
  "version": "0.0.4",
  "description": "Gebruik de pager component om het aantal beschikbare pagina's weer te geven, markeer de huidige pagina en voeg navigatie knoppen toe.",
  "scripts": {
    "prepare": "npm update --dev vl-ui-util && npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "pretest": "npm install chromedriver --chromedriver-force-download",
    "test": "wct -l chrome,firefox --npm",
    "test:grid": "echo TODO",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-pager.js -- *.src.js",
    "release:prepare:imports": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-pager.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-pager/style.css' vl-pager.js",
    "release:prepare:commit": "git add -f vl-pager.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-pager.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-pager/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-pager#readme",
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "dependencies": {
    "vl-ui-core": "^3.0.0"
  },
  "peerDependencies": {
    "vl-ui-core": "^3.0.0"
  },
  "devDependencies": {
    "@govflanders/vl-ui-pager": "^3.9.1",
    "@govflanders/vl-ui-util": "^3.9.1",
    "np": "3.0.4",
    "uglify-es": "3.3.9",
    "vl-ui-util": "^3.2.3"
  }
}
